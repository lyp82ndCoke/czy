.task-detail{
    padding-bottom: 150rpx;
}
.task-user-areainfo{
    padding: 30rpx 61rpx;
    background-color: #FFECE6;
}
.user-icon{
    margin-right: 23rpx;
    font-size: 0;
}
.user-icon image{
    width:106rpx;
    height:106rpx;
    border-radius:50%;
}
.user-message{
    display: flex;
    justify-content: start;
    flex-direction: column;
    width: 100%;
}
.user-nickname{
    font-weight:bold;
    color:rgba(51,51,51,1);
    font-size:36rpx;
    line-height: 1;
}
.query-rank{
    width:127rpx;
    height:34rpx;
    background:rgba(230,0,57,1);
    border-radius:17rpx;
    text-align: center;
    line-height: 34rpx;
    color: #fff;
    font-size:20rpx;
    font-weight:400;
    color:rgba(255,255,255,1);
}
.task-progress-wrap{
    /* padding:0 38rpx 0 54rpx; */
}
.task-progress-title{
    text-align: right;
    color: rgba(153, 153, 153, 1);
    font-size: 20rpx;
}
.task-progress-text{
    font-size:20rpx;
    font-weight:400;
    color:#333;
}
.task-review {
	border: 1px solid transparent;
	border-radius: 5px;
	color: #777;
	display: flex;
	font-size: 12px;
    align-items: center;
}
.task-progress {
    background-color: rgba(100, 100, 100, 0.2);
	border-radius: 11rpx;
	position: relative;
	height: 22rpx;
	width: 100%;
}
.task-progress-done {
    background-color: #FF9951;
    border-radius:11rpx;
	height: 22rpx;
	width: 0;
    transition: width 1s ease 0.3s;
    -ms-transition: width 1s ease 0.3s;
    -webkit-transition: width 1s ease 0.3s;
    -moz-transition: width 1s ease 0.3s;
}
.task-user-data{
    display: flex;
    width: 100%;
    margin-top: 39rpx;
}
.task-user-item{
    flex: 1;
    text-align: left;
    text-align: center;
}
.item-count-sty{
    font-size:32rpx;
    font-weight:500;
    color:rgba(51,51,51,1);
}
.item-name-sty{
    font-size:24rpx;
    font-weight:400;
    color:rgba(102,102,102,1);
}

.task-detail-content{
    padding: 39rpx 39rpx 30rpx;
    background-color: #fff;
    margin-top: 20rpx;
}
.user-details{
    font-size:28rpx;
    font-weight:400;
    color:rgba(51,51,51,1);
    line-height: 1.5;
}
.dianzan-wrap{
    width: 102rpx;
    height: 102rpx;
    border-radius: 50%;
    border: 1rpx solid rgba(153,153,153,1);
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    margin: 39rpx auto 0;
    color:rgba(153,153,153,1);
}
.dianzan-text-sty{
    font-size:20rpx;
    font-weight:400;
    margin-top: 5rpx;
}
.dianzan-sty{
    font-size: 45rpx;
}
.active-zan{
    background-color: #EDEDED;
    color: #E60039;
    border:none;
}
.view-comment{
    display: flex;
    justify-content: flex-end;
    align-items: center;
    flex-direction: row;
    font-size:20rpx;
    font-weight:400;
    color:rgba(153,153,153,1);
}
.liulan-sty{
    font-size: 30rpx;
    margin-right: 10rpx;
}
.pinlun-sty{
    font-size: 30rpx;
    margin: 0 10rpx 0 30rpx;
}
.comment-list{
    padding: 29rpx 40rpx 0;
    background-color: #fff;
    margin-top: 20rpx;
}
.comment-title{
    height: 27rpx;
    line-height: 27rpx;
    border-left: 4px solid #FF9951;
    padding-left: 20rpx;
    font-size:32rpx;
    font-weight:500;
    color:rgba(51,51,51,1);
    margin-bottom: 28rpx;
}
.comment-user-icon{
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}
.comment-nick-name{
    font-size:28rpx;
    font-weight:400;
    color:rgba(102,102,102,1);
}
.comment-top{
    display: flex;
    justify-content: flex-start;
}
.comment-item{
    margin-top: 20rpx;
    padding-bottom: 23rpx;
    border-bottom: 1rpx solid #DDDDDD;
}
.comment-item:first-child{
    margin-top: 0rpx !important;
}
.comment-item:last-child{
    border-bottom: none !important;
    /* padding-bottom: 0 !important; */
}
.comment-text-top{
    font-size:28rpx;
    font-weight:400;
    color:rgba(51,51,51,1);
}
.level-wrap{
    padding: 20rpx 21rpx;
    background-color: #F8F8F8;
    margin-top: 19rpx;
}
.level-item{
    font-size:24rpx;
    font-weight:400;
    color:rgba(51,51,51,1);
    background:rgba(248,248,248,1);
    margin-top: 10rpx;
    /* padding: 5px 0 5px 10px; */
}
.level-item:first-child{
    margin-top: 0rpx !important;
}
.comment-nickname{
    color:#FF9951;
}
